{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/", "destination": "/src/auth/index.html" },
    { "source": "/login", "destination": "/src/auth/login.html" },
    {
      "source": "/marketplace",
      "destination": "/src/marketplace/market-place.html"
    },
    { "source": "/selling", "destination": "/src/selling/sellingpage.html" },
    { "source": "/listing", "destination": "/src/listing/listing.html" },
    { "source": "/wishlist", "destination": "/src/wishlist/wishlist.html" },
    { "source": "/contact", "destination": "/src/contact/contact.html" },
    { "source": "/forum", "destination": "/src/forum/forum.html" },
    { "source": "/terms", "destination": "/src/privacy/terms.html" },
    { "source": "/privacy", "destination": "/src/privacy/privacy.html" },
    { "source": "/complete-profile", "destination": "/src/auth/complete-profile.html" },
    { "source": "/(.*)", "destination": "/$1" }
  ]
}
