{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/",
      "destination": "/src/auth/index.html"
    },
    {
      "source": "/src/marketplace",
      "destination": "/src/marketplace/market-place.html"
    },
    {
      "source": "/src/marketplace/",
      "destination": "/src/marketplace/market-place.html"
    }
  ]
}
